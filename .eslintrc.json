{
  "extends": [
    "next/core-web-vitals"
  ],
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": "warn",
    "react/react-in-jsx-scope": "off",
    "react/prop-types": "off",
    "no-html-link-for-pages": "off",
    "no-img-element": "off"
  }
}
